{
  "name": "packages-smoke",
  "version": "15.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:dev": "ng e2e --configuration=development",
    "e2e:ci": "ng e2e --configuration=production",
    "e2e:headless": "cypress run --headless --browser electron",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev:ssr": "ng run packages-smoke:serve-ssr",
    "serve:ssr": "node dist/packages-smoke/server/main.js",
    "build:ssr": "ng build --configuration=production && ng run packages-smoke:server:production",
    "prerender": "ng run packages-smoke:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.3",
    "@angular/cdk": "^20.1.3",
    "@angular/common": "^20.1.3",
    "@angular/compiler": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@angular/forms": "^20.1.3",
    "@angular/platform-browser": "^20.1.3",
    "@angular/platform-browser-dynamic": "^20.1.3",
    "@angular/platform-server": "^20.1.3",
    "@angular/router": "^20.1.3",
    "@angular/ssr": "^20.1.3",
    "@nebular/auth": "file:.lib/auth",
    "@nebular/date-fns": "file:.lib/date-fns",
    "@nebular/eva-icons": "file:.lib/eva-icons",
    "@nebular/moment": "file:.lib/moment",
    "@nebular/security": "file:.lib/security",
    "@nebular/theme": "file:.lib/theme",
    "date-fns": "^2.25.0",
    "eva-icons": "^1.1.3",
    "express": "^4.15.2",
    "moment": "^2.22.2",
    "rxjs": "^7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.3",
    "@angular-eslint/builder": "~19.1.0",
    "@angular-eslint/eslint-plugin": "~19.1.0",
    "@angular-eslint/eslint-plugin-template": "~19.1.0",
    "@angular-eslint/schematics": "~19.1.0",
    "@angular-eslint/template-parser": "~19.1.0",
    "@angular/cli": "^20.1.3",
    "@angular/compiler-cli": "^20.1.3",
    "@schematics/angular": "^20.1.3",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.10.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "browser-sync": "^3.0.0",
    "eslint": "^8.57.0",
    "jasmine-core": "^3.10.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "cypress": "^13.15.1",
    "@cypress/schematic": "^2.5.2",
    "ts-node": "^10.4.0",
    "typescript": "~5.8.3",
    "webpack-sources": "^3.2.1"
  }
