{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "nebular"
  },
  "apps": [
    {
      "name": "playground",
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "favicon.png"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.css",
        "../node_modules/typeface-exo/index.css",
        "../node_modules/open-sans-fontface/open-sans.css",
        "../node_modules/nebular-icons/scss/nebular-icons.scss",
        "styles.scss"
      ],
      "scripts": [
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "stylePreprocessorOptions": {
        "includePaths": [
          "framework"
        ]
      }
    },
    {
      "name": "playground-wp",
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.wp.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.css",
        "../node_modules/typeface-exo/index.css",
        "../node_modules/open-sans-fontface/open-sans.css",
        "../node_modules/nebular-icons/scss/nebular-icons.scss",
        "styles.scss"
    ],
      "scripts": [
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "stylePreprocessorOptions": {
        "includePaths": [
          ".lib"
        ]
      }
    },
    {
      "name": "docs",
      "root": "docs",
      "outDir": "docs/dist",
      "assets": [
        "assets",
        "favicon.ico",
        "favicon.png",
        "google46533d2e7a851062.html"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/scss/bootstrap-reboot.scss",
        "../node_modules/socicon/css/socicon.css",
        "../node_modules/highlight.js/styles/dracula.css",
        "./app/@theme/styles/styles.scss"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    },
    {
      "project": "docs/tsconfig.spec.json"
    },
    {
      "project": "docs/tsconfig.app.json"
    },
    {
      "project": "scripts/gulp/tsconfig.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
